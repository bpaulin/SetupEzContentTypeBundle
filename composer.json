{
    "name": "bpaulin/setup-ez-content-type-bundle",
    "description": "a bundle to create ezpublish content types from symfony config",
    "authors": [
        {
            "name": "Bruno Paulin",
            "email": "brunopaulin@bpaulin.net"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Bpaulin\\SetupEzContentTypeBundle\\" : "src/" }
    },
    "require": {
        "ezsystems/ezpublish-kernel": "*"
    },
    "require-dev": {
        "ezsystems/ezcs": "dev-master",
        "squizlabs/php_codesniffer": "^2.3",
        "phing/phing": "^2.11",
        "phpunit/phpunit": "^4.7",
        "matthiasnoback/symfony-config-test": "^1.1",
        "phpdocumentor/phpdocumentor": "^2.8",
        "phploc/phploc": "^2.1"
    }
}
